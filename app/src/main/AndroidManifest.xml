<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.njjeske.cs668androiddiabetesapp">

    <uses-sdk
        android:maxSdkVersion="25"
        android:minSdkVersion="19"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".EditActivity"
                android:label="Edit an Activity"
                android:parentActivityName=".Home"/>
        <activity
            android:name=".Results"
            android:label="Results"
            android:parentActivityName=".SearchActivity" />
        <activity
            android:name=".SearchActivity"
            android:label="Search"
            android:parentActivityName=".Home" />
        <activity
            android:name=".AddActivity"
            android:label="Add an Activity"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Regimen"
            android:label="Regimen"
            android:parentActivityName=".Home" />
        <activity
            android:name=".About"
            android:label="About"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Create"
            android:label="Create"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Home"
            android:label="Home"
            android:parentActivityName=".Login" />
        <activity
            android:name=".AddRegimen"
            android:label="Add a Regimen"
            android:parentActivityName=".Regimen" />
        <activity
            android:name=".EditRegimen"
            android:label="Edit Regimen"
            android:parentActivityName=".Regimen" />

    </application>

</manifest>